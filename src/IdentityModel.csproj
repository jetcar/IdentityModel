<Project Sdk="Microsoft.NET.Sdk">
	<PropertyGroup>
		<Authors>Dominick Baier;Brock Allen</Authors>
		<Description>OpenID Connect &amp; OAuth 2.0 client library</Description>
		<PackageId>IdentityModelCore</PackageId>
		<PackageTags>OAuth2;OAuth 2.0;OpenID Connect;Security;Identity;IdentityServer</PackageTags>
		<PackageLicenseExpression>Apache-2.0</PackageLicenseExpression>
		<Nullable>enable</Nullable>
		<LangVersion>latest</LangVersion>
		<TargetFramework>net7.0</TargetFramework>
		<OutputPath>../publish</OutputPath>
		<BaseOutputPath>../publish</BaseOutputPath>
		<GeneratePackageOnBuild>True</GeneratePackageOnBuild>
		<PackageProjectUrl>https://github.com/jetcar/IdentityModel</PackageProjectUrl>
		<RepositoryUrl>https://github.com/jetcar/IdentityModel</RepositoryUrl>
		<PackageReadmeFile>README.md</PackageReadmeFile>
		<Version>1.0.1</Version>

	</PropertyGroup>
	<ItemGroup>
	  <None Include="..\README.md">
	    <Pack>True</Pack>
	    <PackagePath>\</PackagePath>
	  </None>
	</ItemGroup>
	<ItemGroup>
	  <None Update="README.md">
	    <CopyToOutputDirectory>Always</CopyToOutputDirectory>
	  </None>
	</ItemGroup>



</Project>